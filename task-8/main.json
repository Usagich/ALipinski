{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appid": {
            "type": "string"
        },
        "appvalue": {
            "type": "securestring"
        }
    },
    "variables": {
        "link": "https://raw.githubusercontent.com/AzureLabDevOps/ALipinski/master/task-8"
    },
    "resources": [
        {
            "apiVersion": "2017-05-10",
            "name": "kbs",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(variables('link'), '/kbs.json')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "appid": "[parameters('appid')]",
                    "appvalue": "[parameters('appvalue')]"
                    
                }
            }
        }
    ]
}